	number = {3},
	journaltitle = {Addiction (Abingdon, England)},
	shortjournal = {Addiction},
	author = {Boer, Maartje and van den Eijnden, Regina J.J.M. and Finkenauer, Catrin and Boniel-Nissim, Meyran and Marino, Claudia and Inchley, Jo and Cosma, Alina and Paakkari, Leena and Stevens, Gonneke W.J.M.},
