	date = {2022-11},
	note = {Num Pages: 1023-1026
Place: Heidelberg, Netherlands},
	keywords = {Access to information, Body image, Communication, Computer-mediated communication, Coping, Digital media, Information society, Mental health, Online social support, Psychological factors, Self image, Social, Social media, Social networks, Social support, Stress, Virtual communities},
	file = {Full Text PDF:/Users/sarawu/Zotero/storage/GGY2IPZ5/Mikal - 2022 - Where form meets function the impact of social media platform characteristics on access to online s.pdf:application/pdf},
