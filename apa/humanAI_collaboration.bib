@thesis{qiu_modeling_2025,
	location = {United States -- California},
	title = {Modeling Common Ground: Representation, Alignment, and Negotiation in Language and Visual Communications},
	rights = {Database copyright {ProQuest} {LLC}; {ProQuest} does not claim copyright in the individual underlying works.},
	isbn = {979-8-3101-4769-0},
	url = {https://www.proquest.com/docview/3180533656/abstract/EFA58A98E51547C1PQ/1},
	shorttitle = {Modeling Common Ground},
	abstract = {Communication is a dynamic process through which individuals establish and refine their shared understanding, known as common ground. Common ground consists of the mutual knowledge, beliefs, and assumptions that facilitate effective interactions. It can be classified into two primary types: personal common ground, which is the knowledge or experiences shared through direct interactions or a shared history between individuals, and communal common ground, which is the general knowledge shared by individuals due to their membership in the same community, culture, or group. The evolution of common ground occurs through two fundamental mechanisms—alignment and negotiation—allowing individuals to reconcile divergent beliefs and expand their shared understanding. Despite its significance in human communication, modeling common ground computationally poses several challenges, including its dynamic nature, multimodal manifestations, and subjective interpretations.
This thesis investigates the computational representation of common ground and proposes models that infer and utilize it across different modalities. First, a Representational Theory-of-Mind framework is introduced to model belief updates and predict the emergence of common ground in video-based gaze communication. Second, an extension of this framework is applied to dyadic dialogues, integrating explicit mind inference mechanisms into language models to enhance common ground alignment and negotiation. Third, the thesis explores the emergence of graphical conventions as a form of communal common ground, employing reinforcement learning to simulate visual communication games and analyze the evolution of linguistic properties in graphical representations.
By advancing computational models of common ground, this work contributes to a deeper understanding of how shared knowledge is dynamically constructed and offers insights into the integration of artificial intelligence systems into human communicative environments.},
	pagetotal = {124},
	institution = {University of California, Los Angeles},
	type = {phdthesis},
	author = {Qiu, Shuwen},
	urldate = {2025-04-22},
	date = {2025},
	keywords = {Artificial intelligence, Common ground, Communication, Computer engineering, Fundamental mechanisms, Human communicative environments, Language, Mutual knowledge, Subjective interpretations},
	file = {Full Text PDF:/Users/sarawu/Zotero/storage/UXVPDTXS/Qiu - 2025 - Modeling Common Ground Representation, Alignment, and Negotiation in Language and Visual Communicat.pdf:application/pdf},
}

@article{simonton_defining_2018,
	title = {Defining Creativity: Don't We Also Need to Define What Is Not Creative?},
	volume = {52},
	issn = {2162-6057},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/jocb.137},
	doi = {10.1002/jocb.137},
	shorttitle = {Defining Creativity},
	abstract = {I argue that any attempt to define creative ideas cannot fully succeed without also defining uncreative ideas. This argument begins by defining three parameters that characterize a potentially creative thought: the idea's initial probability (p), the final utility (u), and the creator's prior knowledge of that utility (v). The three parameters then lead to a three-criterion multiplicative definition of personal creativity, namely, c = (1 − p)u(1 − v), where the first factor indicates originality and the third factor surprise. Although creativity can only maximize as originality, utility, and surprise all approach unity, the same definition indicates that there are seven different ways that creativity can minimize. These alternatives were identified as (a) routine, reproductive, or habitual ideas, (b) fortuitous response bias, (c) irrational perseveration, (d) problem finding, (e) rational suppression, (f) irrational suppression, and (g) blissful ignorance. If the third parameter v is omitted, then the number of creative and noncreative outcomes reduces to just four, making creativity indistinguishable from irrational suppression. The alternative outcomes are then illustrated using the classic two-string problem. Besides providing a more finely differentiated conception of creativity failures, the definition has critical implications regarding the processes and procedures required to generate highly creative ideas.},
	pages = {80--90},
	number = {1},
	journaltitle = {The Journal of Creative Behavior},
	author = {Simonton, Dean Keith},
	urldate = {2025-04-22},
	date = {2018},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jocb.137},
	keywords = {creativity, originality, prior knowledge, probability, surprise, utility},
	file = {Full Text PDF:/Users/sarawu/Zotero/storage/YJQQ2RTH/Simonton - 2018 - Defining Creativity Don't We Also Need to Define What Is Not Creative.pdf:application/pdf;Snapshot:/Users/sarawu/Zotero/storage/HE4S6WUW/jocb.html:text/html},
}

@article{gilhooly_ai_2024,
	title = {{AI} vs humans in the {AUT}: Simulations to {LLMs}},
	volume = {34},
	issn = {27133745},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S2713374523000304},
	doi = {10.1016/j.yjoc.2023.100071},
	shorttitle = {{AI} vs humans in the {AUT}},
	abstract = {This paper reviews studies of proposed creative machines applied to a prototypical creative task, i.e., the Alternative Uses Task ({AUT}). Although one system ({OROC}) did simulate some aspects of human strategies for the {AUT}, most recent attempts have not been simulation-oriented, but rather have used Large Language Model ({LLM}) systems such as {GPT}-3 which embody extremely large connectionist networks trained on huge volumes of textual data. Studies reviewed here indicate that {LLM} based systems are performing on the {AUT} at near or somewhat above human levels in terms of scores on originality and usefulness. Moreover, similar patterns are found in the data of humans and {LLM} models in the {AUT}, such as output order effects and a negative association between originality and value or utility. However, it is concluded that {GPT}-3 and similar systems, despite generating novel and useful responses, do not display creativity as they lack agency and are purely algorithmic. {LLM} studies so far in this area have largely been exploratory and future studies should guard against possible training data contamination.},
	pages = {100071},
	number = {1},
	journaltitle = {Journal of Creativity},
