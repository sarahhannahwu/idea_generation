	date = {2016},
	note = {Place: {US}},
	keywords = {Addiction, Attention Deficit Disorder with Hyperactivity, Computer Games, Digital Gaming, Internet Addiction, Mental Disorders, Online Social Networks, Social Media, Symptoms},
