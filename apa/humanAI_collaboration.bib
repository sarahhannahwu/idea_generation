@article{silvia_discernment_2008,
	title = {Discernment and creativity: How well can people identify their most creative ideas?},
	volume = {2},
	issn = {1931-390X},
	doi = {10.1037/1931-3896.2.3.139},
	shorttitle = {Discernment and creativity},
	abstract = {How well can people judge the creativity of their ideas? The distinction between generating ideas and evaluating ideas appears in many theories of creativity, but the massive literature on generation has overshadowed the question of evaluation. After critically reviewing the notion of accuracy in creativity judgments, this article explores whether (1) people in general are discerning and (2) whether some people are more discerning than others. University students (n = 226) completed four divergent thinking tasks and then decided which responses were their most creative. Judges then rated the creativity of all of the responses. Multilevel latent-variable models found that people's choices strongly agreed with judges' ratings of the responses; overall, people were discerning in their decisions. But some people were more discerning than others: people high in openness to experience, in particular, had stronger agreement between their decisions and the judges' ratings. Creative people are thus doubly skilled: they are better at generating good ideas and at picking their best ideas. ({PsycInfo} Database Record (c) 2023 {APA}, all rights reserved)},
