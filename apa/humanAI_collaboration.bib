	keywords = {Creativity, {AI}-augmented innovation, Artificial intelligence, artificial intelligence ({AI}), Chatbots, {chatGPT}, Companies, creativity, generative {AI}, Generative {AI}, idea generation, innovation, large language models ({LLMs}), Task analysis, Technological innovation},
	file = {Full Text PDF:/Users/sarawu/Zotero/storage/DZ97U6AI/Joosten et al. - 2024 - Comparing the Ideation Quality of Humans With Generative Artificial Intelligence.pdf:application/pdf;IEEE Xplore Abstract Record:/Users/sarawu/Zotero/storage/ME4XN6ZQ/10398283.html:text/html},
